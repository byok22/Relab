    <PackageReference Include="GraphQL.SystemTextJson" Version="7.8.0" />
    <PackageReference Include="NATS.Client" Version="1.1.7-pre1" />
    <PackageReference Include="NATS.Client.Core" Version="2.3.2" />
    <PackageReference Include="NATS.Client.Serializers.Json" Version="2.3.2" />
    <PackageReference Include="NATS.Net" Version="2.3.2" />
  </ItemGroup>
  <ItemGroup>
  <None Update="Nats\nats-server.exe">
